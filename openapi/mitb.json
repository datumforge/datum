          {
            "type": "string",
            "x-go-name": "Username",
            "name": "username",
            "in": "query"
          },
          {
            "type": "string",
            "x-go-name": "Password",
            "name": "password",
            "in": "query"
          },
          {
            "type": "string",
            "x-go-name": "InviteToken",
            "name": "invite_token",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/responses/LoginReply"
          },
          "400": {
            "$ref": "#/responses/BadRequest"
          },
          "500": {
            "$ref": "#/responses/InternalServerError"
          }
        }
      }
    }
  },
